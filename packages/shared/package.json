{
  "name": "@voide/shared",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "proto:gen": "protoc --proto_path ../../proto --plugin=protoc-gen-ts=../../node_modules/.pnpm/node_modules/.bin/protoc-gen-ts_proto --ts_out src/gen --ts_opt generate_dependencies,importSuffix=.js,outputServices=generic-definitions,esModuleInterop=true ../../proto/flow.proto"
  },
  "engines": { "node": ">=20.11" },
  "dependencies": {
    "protobufjs": "^7.2.3"
  },
  "devDependencies": {
    "ts-proto": "^1.169.0"
  }
}


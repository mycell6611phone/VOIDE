{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "verbatimModuleSyntax": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@voide/shared": [
        "packages/shared/src/index.ts",
        "packages/shared/dist/index.js"
      ],
      "@voide/shared/*": [
        "packages/shared/src/*",
        "packages/shared/dist/*"
      ],
      "@voide/schemas": [
        "packages/schemas/src/index.ts",
        "packages/schemas/dist/index.js"
      ],
      "@voide/schemas/*": [
        "packages/schemas/src/*",
        "packages/schemas/dist/*"
      ],
      "@voide/adapters": [
        "packages/adapters/src/index.ts",
        "packages/adapters/src/index.js",
        "packages/adapters/dist/index.js",
        "packages/adapters/src"
      ],
      "@voide/adapters/*": [
        "packages/adapters/src/*",
        "packages/adapters/dist/*"
      ],
      "@voide/workers": [
        "packages/workers/src/index.ts",
        "packages/workers/src/index.js",
        "packages/workers/dist/index.js",
        "packages/workers/src"
      ],
      "@voide/workers/*": [
        "packages/workers/src/*",
        "packages/workers/dist/*"
      ],
      "@voide/ipc": [
        "packages/ipc/src/index.ts",
        "packages/ipc/dist/index.js"
      ],
      "@voide/ipc/*": [
        "packages/ipc/src/*",
        "packages/ipc/dist/*"
      ]
    }
  }
}
